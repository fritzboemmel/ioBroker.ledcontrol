		"icon": "ledcontrol.png",
		"enabled": true,
		"extIcon": "https://raw.githubusercontent.com/fritzboemmel/ioBroker.ledcontrol/master/admin/ledcontrol.png",
		"readme": "https://github.com/fritzboemmel/ioBroker.ledcontrol/blob/master/README.md",
		"loglevel": "info",
		"mode": "daemon",
		"type": "lighting",
		"compact": true,
		"connectionType": "local",
		"dataSource": "push",
		"materialize": true,
		"supportCustoms": true,
		"dependencies": [
			{
				"js-controller": ">=2.0.0"
			}
		]
	},
	"native": {
		"ipadress": "192.168.1.100",
		"username": "benutzername",
		"password": "password",
		"port": 1234
	},
	"objects": [],
	"instanceObjects": [
		{
			"_id": "info",
			"type": "channel",
			"common": {
				"name": "Information"
			},
			"native": {}
		},
		{
			"_id": "info.connection",
			"type": "state",
			"common": {
				"role": "indicator.connected",
				"name": "Device or service connected",
				"type": "boolean",
				"read": true,
				"write": false,
				"def": false
			},
			"native": {}
		},
		{
            "_id": "ledcontrol.meta",
            "type": "meta",
            "meta": {
                "adapter": "ledcontrol",
                "type": ""
            },
            "common": {},
            "native": {}
        },
        {
            "_id": "ledcontrol.meta.switch",
            "type": "meta",
            "parent": "ledcontrol.meta",
            "meta": {
                "adapter": "ledcontrol",
                "type": "fixture"
            },
            "common": {
                "name": "generic switch"
            },
            "native": {
                "channel": {
                    "type": "channel",
                    "common": {
                        "role": "switch"
                    },
                    "native": {}
                },
                "length": 1,
                "states": [
                    {
                        "type": "state",
                        "common": {
                            "role": "switch",
                            "type": "boolean",
                            "write": true,
                            "read": false
                        },
                        "native": {
                            "value_off": 0,
                            "value_on": 255
                        }
                    }
                ]
            }
        },
        {
            "_id": "ledcontrol.meta.dimmer",
            "type": "meta",
            "parent": "ledcontrol.meta",
            "meta": {
                "adapter": "ledcontrol",
                "type": "fixture"
            },
            "common": {
                "name": "generic dimmer"
            },
            "native": {
                "channel": {
                    "type": "channel",
                    "common": {
                        "role": "light.dimmer"
                    },
                    "native": {}
                },
                "length": 1,
                "states": [
                    {
                        "type": "state",
                        "common": {
                            "role": "level.dimmer",
                            "type": "number",
                            "min": 0,
                            "max": 255,
                            "write": true,
                            "read": false
                        },
                        "native": {}
                    }
                ]
            }
        },
        {
            "_id": "ledcontrol.meta.rgb",
            "type": "meta",
            "parent": "ledcontrol.meta",
            "meta": {
                "adapter": "ledcontrol",
                "type": "fixture"
            },
            "common": {
                "name": "generic rgb"
            },
            "native": {
                "channel": {
                    "type": "channel",
                    "common": {
                        "role": "light.rgb"
                    },
                    "native": {}
                },
                "length": 3,
                "states": [
                    {
                        "type": "state",
                        "common": {
                            "role": "level.color.red",
                            "type": "number",
                            "min": 0,
                            "max": 255,
                            "write": true,
                            "read": false
                        },
                        "native": {}
                    },
                    {
                        "type": "state",
                        "common": {
                            "role": "level.color.green",
                            "type": "number",
                            "min": 0,
                            "max": 255,
                            "write": true,
                            "read": false
                        },
                        "native": {}
                    },
                    {
                        "type": "state",
                        "common": {
                            "role": "level.color.blue",
                            "type": "number",
                            "min": 0,
                            "max": 255,
                            "write": true,
                            "read": false
                        },
                        "native": {}
                    },
                    {
                        "type": "state",
                        "common": {
                            "role": "level.color.rgb",
                            "type": "string",
                            "write": true,
                            "read": false
                        },
                        "native": {}
                    }
                ]
            }
        }
	]
}